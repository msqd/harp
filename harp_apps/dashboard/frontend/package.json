{
  "name": "harp-dashboard",
  "version": "0.7.0",
  "type": "module",
  "private": true,
  "description": "Harp Dashboard",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default":"./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks.js",
      "default":"./dist/hooks.js",
      "require": "./dist/hooks.cjs",
      "types": "./dist/hooks.d.ts"
    },
    "./ui": {
      "import": "./dist/ui.js",
      "default":"./dist/ui.js",
      "require": "./dist/ui.cjs",
      "types": "./dist/ui.d.ts"
    },
    "./tests/handlers":{
      "import": "./dist/handlers.js",
      "default":"./dist/handlers.js",
      "require": "./dist/handlers.cjs",
      "types": "./dist/handlers.d.ts"
    }
    },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "ui:build": "ladle build",
    "ui:preview": "ladle build && pnpm ui:serve:prod",
    "ui:serve:prod": "ladle preview -p 61110",
    "ui:serve:dev": "ladle serve -p 61110",
    "ui:serve": "pnpm ui:serve:dev",
    "test:ui:dev": "cross-env TYPE=dev LANG=en_US.UTF-8 pnpm exec playwright test -c src/ui/playwright.config.ts",
    "test:ui:prod": "cross-env TYPE=prod LANG=en_US.UTF-8 pnpm exec playwright test -c src/ui/playwright.config.ts",
    "test:ui:update": "cross-env TYPE=update LANG=en_US.UTF-8 pnpm exec playwright test -c src/ui/playwright.config.ts -u",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "test:unit": "cross-env LANG=en_US.UTF-8 vitest run",
    "test:unit:update": "cross-env LANG=en_US.UTF-8 vitest run --update",
    "test:browser": "cross-env LANG=en_US.UTF-8 playwright test",
    "test:coverage": "cross-env LANG=en_US.UTF-8 vitest run --coverage",
    "test": "pnpm run test:unit && pnpm run test:browser"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "dependencies": {
    "@bramus/pagination-sequence": "^1.2.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@headlessui/react": "2.1.5",
    "@heroicons/react": "^2.1.5",
    "@ladle/react": "^4.1.1",
    "@react-hook/window-size": "^3.1.1",
    "@sentry/browser": "^8.30.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/lodash": "^4.17.7",
    "@types/react-helmet": "^6.1.11",
    "date-fns": "^3.6.0",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "http-status-codes": "^2.3.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.4",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-helmet": "^6.1.0",
    "react-is": "18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.26.2",
    "react-syntax-highlighter": "^15.5.0",
    "sort-by": "^1.2.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.2",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.2.1",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@playwright/test": "^1.47.1",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/bramus__pagination-sequence": "^1.2.2",
    "@types/node": "^22.5.5",
    "@types/prismjs": "^1.26.4",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@types/react-sparklines": "^1.7.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/sync-fetch": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.1",
    "autoprefixer": "^10.4.20",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-prismjs": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-autofix": "^2.2.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "jsdom": "^24.1.3",
    "json-schema-to-typescript": "^13.1.2",
    "msw": "^2.4.8",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sync-fetch": "^0.5.2",
    "tailwindcss": "^3.4.11",
    "twin.macro": "^3.4.1",
    "typescript": "5.4.3",
    "vite": "^5.4.6",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.1",
    "vitest-canvas-mock": "^0.3.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.21.2"
  }
}
